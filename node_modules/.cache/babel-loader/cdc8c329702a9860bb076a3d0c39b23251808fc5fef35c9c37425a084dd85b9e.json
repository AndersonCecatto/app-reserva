{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport api from '@/services/api';\nimport LoadComponent from './LoadComponent.vue';\nimport AlertComponent from './AlertComponent.vue';\nexport default {\n  components: {\n    AlertComponent,\n    LoadComponent\n  },\n  name: \"HistoricoComponent\",\n  data: () => ({\n    alertHistorico: false,\n    textoAlerta: \"\",\n    tipoAlerta: \"\",\n    loader: false,\n    sortBy: 'Data',\n    sortDesc: false,\n    search: '',\n    headers: [{\n      text: 'Restaurante',\n      align: 'start',\n      value: 'Empresa'\n    }, {\n      text: 'Data',\n      value: 'Data'\n    }, {\n      text: 'Horário/Periodo',\n      value: 'Horario'\n    }, {\n      text: 'Status',\n      value: 'Status'\n    }],\n    historicos: []\n  }),\n  methods: {\n    getColor(status) {\n      if (status == 'Cancelada') return 'red';else if (status == 'Em Andamento') return 'yellow';else return 'green';\n    },\n    EnableAlert(texto, tipoAlerta) {\n      this.alertHistorico = true;\n      this.tipoAlerta = tipoAlerta;\n      this.textoAlerta = texto;\n      setTimeout(() => {\n        this.alertHistorico = false;\n      }, 1800);\n    }\n  },\n  created() {\n    this.loader = !this.loader;\n    api.get('Reserva/' + 3).then(response => {\n      response.data.forEach(element => {\n        this.historicos.push({\n          Id: element.id,\n          Empresa: element.empresa.nome,\n          Data: new Date(element.dataReserva).getDay().toString().padStart(2, '0') + '/' + new Date(element.dataReserva).getMonth().toString().padStart(2, '0') + '/' + new Date(element.dataReserva).getFullYear(),\n          Horario: new Date(element.horario).getHours() + ':' + new Date(element.horario).getMinutes(),\n          Status: element.ativo ? 'Concluida' : 'Cancelada'\n        });\n      });\n    }).catch(error => {\n      this.EnableAlert(error.message, \"error\");\n      window.scrollTo(0, 0);\n      console.log(error);\n    }).finally(() => this.loader = !this.loader);\n  }\n};","map":{"version":3,"mappings":";AA4DA;AACA;AACA;AAEA;EACAA;IAAAC;IAAAC;EAAA;EAEAC;EAEAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC,UACA;MACAC;MACAC;MACAC;IACA,GACA;MACAF;MACAE;IACA,GACA;MACAF;MACAE;IACA,GACA;MACAF;MACAE;IACA,EACA;IACAC;EACA;EAEAC;IACAC;MACA,6CACA,mDACA;IACA;IACAC;MACA;MACA;MACA;MAEAC;QACA;MACA;IACA;EACA;EACAC;IAEA;IAEAC,wBACAC;MACAC;QACA;UACAC;UACAC;UACAC;UACAC;UACAC;QACA;MACA;IACA,GACAC;MACA;MACAC;MACAC;IACA,GACAC;EACA;AACA","names":["components","AlertComponent","LoadComponent","name","data","alertHistorico","textoAlerta","tipoAlerta","loader","sortBy","sortDesc","search","headers","text","align","value","historicos","methods","getColor","EnableAlert","setTimeout","created","api","then","response","Id","Empresa","Data","Horario","Status","catch","window","console","finally"],"sourceRoot":"src/components","sources":["HistoricoClienteComponent.vue"],"sourcesContent":["<template>\r\n    <v-card class=\"pb-3\">\r\n        <alert-component :texto-alerta=\"textoAlerta\" :tipo='tipoAlerta' v-if=\"alertHistorico == true\"/>\r\n        <v-card-title>\r\n            Reservas\r\n            <v-spacer></v-spacer>\r\n            <v-text-field\r\n                v-model=\"search\"\r\n                append-icon=\"mdi-magnify\"\r\n                label=\"Pesquisar\"\r\n                single-line\r\n                hide-details\r\n            ></v-text-field>\r\n        </v-card-title>\r\n        \r\n        <v-data-table\r\n            :search=\"search\"\r\n            :headers=\"headers\"\r\n            :items=\"historicos\"\r\n            :footer-props=\"{\r\n                'items-per-page-text':'Linhas por pagina'\r\n            }\"\r\n            :header-props=\"{\r\n                'sortByText': 'Ordenar'\r\n            }\"\r\n            :sort-desc.sync=\"sortDesc\"\r\n            :sort-by.sync=\"sortBy\"\r\n        >\r\n            <template v-slot:item.Status=\"{ item }\">\r\n                <v-chip\r\n                    :color=\"getColor(item.Status)\"\r\n                    dark\r\n                >\r\n                    {{ item.Status }}\r\n                </v-chip>\r\n            </template>\r\n            <template v-slot:item.Empresa=\"{ item }\">\r\n                <td class=\"font-weight-black\">{{ item.Empresa }}</td>\r\n            </template>\r\n            <!-- <template v-slot:item.actions=\"{ item }\">\r\n                <v-icon\r\n                    small\r\n                    class=\"mr-2\"\r\n                    @click=\"editItem(item)\"\r\n                >\r\n                    mdi-pencil\r\n                </v-icon>\r\n                <v-icon\r\n                    small\r\n                    @click=\"deleteItem(item)\"\r\n                >\r\n                    mdi-delete\r\n                </v-icon>\r\n            </template> -->\r\n        </v-data-table>\r\n        <load-component :Ativo=\"loader\"/>\r\n    </v-card>\r\n</template>\r\n<script>\r\n\r\nimport api from '@/services/api';\r\nimport LoadComponent from './LoadComponent.vue';\r\nimport AlertComponent from './AlertComponent.vue';\r\n\r\nexport default {\r\n    components: { AlertComponent, LoadComponent },\r\n\r\n    name: \"HistoricoComponent\",\r\n\r\n    data: () => ({\r\n        alertHistorico: false,\r\n        textoAlerta: \"\",\r\n        tipoAlerta: \"\",\r\n        loader: false,\r\n        sortBy: 'Data',\r\n        sortDesc: false,\r\n        search: '',\r\n        headers: [\r\n            {\r\n                text: 'Restaurante',\r\n                align: 'start',\r\n                value: 'Empresa',\r\n            },\r\n            {\r\n                text: 'Data',\r\n                value: 'Data',\r\n            },\r\n            {\r\n                text: 'Horário/Periodo',\r\n                value: 'Horario',\r\n            },\r\n            {\r\n                text: 'Status',\r\n                value: 'Status',\r\n            }\r\n        ],\r\n        historicos: []\r\n    }),\r\n\r\n    methods: {\r\n        getColor(status) {\r\n            if (status == 'Cancelada') return 'red'\r\n            else if (status == 'Em Andamento') return 'yellow'\r\n            else return 'green'\r\n        },\r\n        EnableAlert(texto, tipoAlerta) {\r\n            this.alertHistorico = true\r\n            this.tipoAlerta = tipoAlerta\r\n            this.textoAlerta = texto\r\n\r\n            setTimeout(() => {\r\n                this.alertHistorico = false\r\n            }, 1800)\r\n        },\r\n    },\r\n    created() {\r\n\r\n        this.loader = !this.loader;\r\n\r\n        api.get('Reserva/'+3)\r\n        .then(response => {\r\n            response.data.forEach(element => {\r\n               this.historicos.push({\r\n                    Id: element.id,\r\n                    Empresa: element.empresa.nome,\r\n                    Data: new Date(element.dataReserva).getDay().toString().padStart(2, '0')+'/'+new Date(element.dataReserva).getMonth().toString().padStart(2, '0')+'/'+new Date(element.dataReserva).getFullYear(),\r\n                    Horario: new Date(element.horario).getHours()+':'+new Date(element.horario).getMinutes(),\r\n                    Status: element.ativo ? 'Concluida' : 'Cancelada'\r\n               })\r\n            });\r\n        })\r\n        .catch(error => {\r\n            this.EnableAlert(error.message, \"error\")\r\n            window.scrollTo(0,0);\r\n            console.log(error)\r\n        })\r\n        .finally(() => (this.loader = !this.loader))\r\n    }\r\n}\r\n</script>\r\n<style>\r\n    \r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}