{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport GenericMethods from '@/mixins/GenericMethods';\nimport CardPadraoComponent from '../Field/CardPadraoComponent.vue';\nimport RequestMethods from '@/mixins/RequestMethods';\nimport DialogPersistentWithoutBtnComponent from '../Field/DialogPersistentWithoutBtnComponent.vue';\nimport CadastroUsuarioComponent from './CadastroUsuarioComponent.vue';\nimport AlertComponent from '../AlertComponent.vue';\nexport default {\n  components: {\n    CardPadraoComponent,\n    DialogPersistentWithoutBtnComponent,\n    CadastroUsuarioComponent,\n    AlertComponent\n  },\n  mixins: [GenericMethods, RequestMethods, CadastroUsuarioComponent],\n  name: 'ListaUsuariosComponent',\n  data: () => ({\n    headers: [{\n      text: 'Codigo',\n      value: 'Id'\n    }, {\n      text: 'Nome',\n      value: 'Nome'\n    }, {\n      text: 'Data Cadastro',\n      value: 'DataCadastro'\n    }, {\n      text: 'Login',\n      value: 'Login'\n    }, {\n      text: 'Ativo',\n      value: 'Ativo'\n    }],\n    clientes: [],\n    search: '',\n    salvarUsuario: false,\n    cancelarUsuario: false,\n    sortBy: 'Id',\n    sortDesc: true\n  }),\n  methods: {\n    requestUsuarios() {\n      this.RequestGet('Usuario', retorno => this.RetornoUsuarios(retorno), error => this.RetornoErro(error), () => this.loader = !this.loader);\n    },\n    RetornoUsuarios(retorno) {\n      retorno.data.forEach(element => {\n        this.clientes.push({\n          Id: element.id,\n          Nome: element.nome,\n          DataCadastro: this.FormatDate(new Date(element.dataCadastro).toISOString().substring(0, 10)),\n          Login: element.login,\n          Ativo: element.ativo == true ? 'Sim' : 'Não'\n        });\n      });\n    },\n    SalvarUsuario() {\n      this.salvarUsuario = !this.salvarUsuario;\n    },\n    CancelarUsuario() {\n      this.cancelarUsuario = !this.cancelarUsuario;\n      this.dialog = !this.dialog;\n    },\n    retornoDialog(retorno) {\n      debugger;\n      this.dialog = !this.dialog;\n      this.EnableAlert(\"Usuário cadastrado com sucesso.\", \"success\");\n      this.requestUsuarios();\n    }\n  },\n  created() {\n    this.requestUsuarios();\n  }\n};","map":{"version":3,"mappings":";AA6FA;AACA;AACA;AACA;AACA;AACA;AAEA;EACAA;IAAAC;IAAAC;IAAAC;IAAAC;EAAA;EACAC,yCACAF;EACAG;EAEAC;IACAC,UACA;MAAAC;MAAAC;IAAA,GACA;MAAAD;MAAAC;IAAA,GACA;MAAAD;MAAAC;IAAA,GACA;MAAAD;MAAAC;IAAA,GACA;MAAAD;MAAAC;IAAA,EACA;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;EACA;EAEAC;IAEAC;MACA,2BACAC,0CACAC,kCACA;IACA;IAEAC;MACAF;QACA;UACAG;UACAC;UACAC;UACAC;UACAC;QACA;MACA;IACA;IAEAC;MACA;IACA;IAEAC;MACA;MAEA;IACA;IAEAC;MACA;MACA;MAEA;MACA;IACA;EACA;EAEAC;IACA;EACA;AACA","names":["components","CardPadraoComponent","DialogPersistentWithoutBtnComponent","CadastroUsuarioComponent","AlertComponent","mixins","name","data","headers","text","value","clientes","search","salvarUsuario","cancelarUsuario","sortBy","sortDesc","methods","requestUsuarios","retorno","error","RetornoUsuarios","Id","Nome","DataCadastro","Login","Ativo","SalvarUsuario","CancelarUsuario","retornoDialog","created"],"sourceRoot":"src/components/Usuario","sources":["ListarUsuariosComponent.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <alert-component :texto-alerta=\"textoAlerta\" :tipo='tipoAlerta' v-if=\"alerta == true\"/>\r\n        <card-padrao-component>\r\n            <template v-slot:titulo>\r\n                Usuario\r\n                <v-spacer></v-spacer>\r\n                <v-text-field\r\n                    v-model=\"search\"\r\n                    append-icon=\"mdi-magnify\"\r\n                    label=\"Pesquisar\"\r\n                    single-line\r\n                    hide-details\r\n                ></v-text-field>\r\n                <v-spacer></v-spacer>\r\n                <v-btn\r\n                    class=\"mr-3\"\r\n                    outlined\r\n                    large\r\n                    right\r\n                    color=\"primary\"\r\n                    @click=\"dialog = !dialog\"\r\n                    >\r\n                    Novo\r\n                </v-btn>\r\n            </template>\r\n            <template v-slot:texto>\r\n                <v-data-table\r\n                    :headers=\"headers\"\r\n                    :items=\"clientes\"\r\n                    :search=\"search\"\r\n                    :footer-props=\"{\r\n                        'items-per-page-text':'Linhas por pagina',\r\n                        'items-per-page-options': [5, 10, 20, 50]\r\n                    }\"\r\n                    :header-props=\"{\r\n                        'sortByText': 'Ordenar'\r\n                    }\"\r\n                    :sort-desc.sync=\"sortDesc\"\r\n                    :sort-by.sync=\"sortBy\"\r\n                >\r\n                    <template v-slot:item.Nome=\"{ item }\">\r\n                        <td class=\"font-weight-black\">{{ item.Nome }}</td>\r\n                    </template>\r\n                    <template v-slot:item.Login=\"{ item }\">\r\n                        <td class=\"font-weight-black\">{{ item.Login }}</td>\r\n                    </template>\r\n                </v-data-table>\r\n            </template>\r\n        </card-padrao-component>\r\n        <dialog-persistent-without-btn-component\r\n            titulo=\"Cadastro Usuário\"\r\n            :dialogNew=\"dialog\"\r\n            tamanho=\"800\"\r\n        >\r\n            <template v-slot:text>\r\n                <cadastro-usuario-component\r\n                    :salvar=\"salvarUsuario\"\r\n                    :cancelar=\"cancelarUsuario\"\r\n                    :clean=\"true\"\r\n                    @response=\"retornoDialog\"\r\n                />\r\n            </template>\r\n            <template v-slot:actions>\r\n                <v-spacer/>\r\n                <v-btn\r\n                    outlined\r\n                    large\r\n                    color=\"error\"\r\n                    @click=\"CancelarUsuario()\"\r\n                    >\r\n                    <v-icon left>\r\n                        mdi-cancel\r\n                    </v-icon>\r\n                    Cancelar\r\n                </v-btn>\r\n                <v-btn\r\n                    outlined\r\n                    large\r\n                    color=\"green\"\r\n                    @click=\"SalvarUsuario()\"\r\n                    >\r\n                    <v-icon left>\r\n                        mdi-check\r\n                    </v-icon>\r\n                    Confirmar\r\n                </v-btn>\r\n            </template>\r\n        </dialog-persistent-without-btn-component>\r\n    </div>\r\n</template>\r\n<script>\r\n\r\nimport GenericMethods from '@/mixins/GenericMethods'\r\nimport CardPadraoComponent from '../Field/CardPadraoComponent.vue'\r\nimport RequestMethods from '@/mixins/RequestMethods'\r\nimport DialogPersistentWithoutBtnComponent from '../Field/DialogPersistentWithoutBtnComponent.vue'\r\nimport CadastroUsuarioComponent from './CadastroUsuarioComponent.vue'\r\nimport AlertComponent from '../AlertComponent.vue'\r\n\r\nexport default {\r\n    components: { CardPadraoComponent, DialogPersistentWithoutBtnComponent, CadastroUsuarioComponent, AlertComponent },\r\n    mixins: [GenericMethods, RequestMethods,\r\n        CadastroUsuarioComponent],\r\n    name: 'ListaUsuariosComponent',\r\n\r\n    data: () => ({\r\n        headers: [\r\n            { text: 'Codigo', value: 'Id' },\r\n            { text: 'Nome', value: 'Nome' },\r\n            { text: 'Data Cadastro', value: 'DataCadastro' },\r\n            { text: 'Login', value: 'Login' },\r\n            { text: 'Ativo', value: 'Ativo' },\r\n        ],\r\n        clientes: [],\r\n        search: '',\r\n        salvarUsuario: false,\r\n        cancelarUsuario: false,\r\n        sortBy: 'Id',\r\n        sortDesc: true,\r\n    }),\r\n\r\n    methods: {\r\n        \r\n        requestUsuarios() {\r\n            this.RequestGet('Usuario',\r\n            (retorno) => this.RetornoUsuarios(retorno), \r\n            (error) => this.RetornoErro(error),\r\n            () => (this.loader = !this.loader))\r\n        },\r\n\r\n        RetornoUsuarios(retorno) {\r\n            retorno.data.forEach(element => {\r\n                this.clientes.push({\r\n                    Id: element.id,\r\n                    Nome: element.nome,\r\n                    DataCadastro: this.FormatDate(new Date(element.dataCadastro).toISOString().substring(0,10)),\r\n                    Login: element.login,\r\n                    Ativo: element.ativo == true ? 'Sim' : 'Não'\r\n                })\r\n            })\r\n        },\r\n\r\n        SalvarUsuario() {\r\n            this.salvarUsuario = !this.salvarUsuario\r\n        },\r\n\r\n        CancelarUsuario() {\r\n            this.cancelarUsuario = !this.cancelarUsuario\r\n\r\n            this.dialog = !this.dialog\r\n        },\r\n\r\n        retornoDialog(retorno) {\r\n            debugger\r\n            this.dialog = !this.dialog\r\n\r\n            this.EnableAlert(\"Usuário cadastrado com sucesso.\", \"success\")\r\n            this.requestUsuarios()\r\n        },\r\n    },\r\n\r\n    created() {\r\n        this.requestUsuarios()\r\n    }\r\n}\r\n</script>\r\n<style>\r\n    \r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}