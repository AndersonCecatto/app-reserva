{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport api from '@/services/api';\nimport LoadComponent from './LoadComponent.vue';\nimport AlertComponent from './AlertComponent.vue';\nimport GenericMethods from '@/mixins/GenericMethods';\nexport default {\n  components: {\n    AlertComponent,\n    LoadComponent\n  },\n  name: \"HistoricoComponent\",\n  mixins: [GenericMethods],\n  data: () => ({\n    alertHistorico: false,\n    textoAlerta: \"\",\n    tipoAlerta: \"\",\n    loader: false,\n    sortBy: 'Id',\n    sortDesc: true,\n    search: '',\n    headers: [{\n      text: 'Codigo',\n      align: 'start',\n      value: 'Id'\n    }, {\n      text: 'Restaurante',\n      align: 'start',\n      value: 'Empresa'\n    }, {\n      text: 'Data',\n      value: 'Data'\n    }, {\n      text: 'Horário/Periodo',\n      value: 'Horario'\n    }, {\n      text: 'Quantidade Pessoas',\n      value: 'Pessoas'\n    }, {\n      text: 'Status',\n      value: 'Status'\n    }, {\n      text: 'Ações',\n      value: 'actions',\n      sortable: false\n    }, {\n      text: '',\n      value: 'data-table-expand'\n    }],\n    historicos: []\n  }),\n  methods: {\n    cancelarReserva(item) {\n      this.motivoCancelamento = null;\n      if (item.Status == 'Cancelada') {\n        this.EnableAlert(\"Reserva já cancelada.\", \"warning\");\n        window.scrollTo(0, 0);\n        return;\n      }\n      this.acao = \"Cancelar\";\n      this.itemEnvio = item;\n      this.dialog = !this.dialog;\n    },\n    requestAlterarStatus(id, cancelar, motivoCancelamento, ativo) {\n      this.loader = !this.loader;\n      api.post('Reserva/AlterarStatus', {\n        id: id,\n        Cancelada: cancelar,\n        MotivoCancelamento: motivoCancelamento,\n        Ativo: ativo\n      }).then(response => {\n        this.EnableAlert(\"Concluido com sucesso.\", \"success\");\n        console.log(response);\n      }).catch(error => {\n        this.EnableAlert(error.message, \"error\");\n        window.scrollTo(0, 0);\n        console.log(error);\n      }).finally(() => {\n        this.loader = !this.loader;\n        this.requestBuscarPorEmpresaId();\n      });\n    }\n  },\n  created() {\n    this.loader = !this.loader;\n    debugger;\n    api.get('Reserva/' + 3).then(response => {\n      response.data.forEach(element => {\n        this.historicos.push({\n          Id: element.id,\n          Empresa: element.empresa.nome,\n          Data: this.FormatDate(new Date(element.dataReserva).toISOString().substring(0, 10)),\n          Horario: new Date(element.horario).getHours() + ':' + new Date(element.horario).getMinutes(),\n          Pessoas: element.quantidadePessoas,\n          Status: element.cancelada ? 'Cancelada' : element.ativo ? \"Concluida\" : \"Aguardando\"\n        });\n      });\n    }).catch(error => {\n      this.EnableAlert(error.message, \"error\");\n      window.scrollTo(0, 0);\n      console.log(error);\n    }).finally(() => this.loader = !this.loader);\n  }\n};","map":{"version":3,"mappings":";AA0DA;AACA;AACA;AACA;AAEA;EACAA;IAAAC;IAAAC;EAAA;EAEAC;EACAC;EACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC,UACA;MAAAC;MAAAC;MAAAC;IAAA,GACA;MAAAF;MAAAC;MAAAC;IAAA,GACA;MAAAF;MAAAE;IAAA,GACA;MAAAF;MAAAE;IAAA,GACA;MAAAF;MAAAE;IAAA,GACA;MAAAF;MAAAE;IAAA,GACA;MAAAF;MAAAE;MAAAC;IAAA,GACA;MAAAH;MAAAE;IAAA,EACA;IACAE;EACA;EAEAC;IACAC;MACA;MAEA;QACA;QACAC;QACA;MACA;MAEA;MACA;MACA;IACA;IACAC;MACA;MAEAC,kCACA;QACAC;QACAC;QACAC;QACAC;MACA,GACAC;QACA;QACAC;MACA,GACAC;QACA;QACAT;QACAQ;MACA,GACAE;QACA;QACA;MACA;IAEA;EACA;EACAC;IAEA;IACA;IAEAT,wBACAK;MACAK;QACA;UACAC;UACAC;UACAC;UACAC;UACAC;UACAC;QACA;MACA;IACA,GACAT;MACA;MACAT;MACAQ;IACA,GACAE;EACA;AACA","names":["components","AlertComponent","LoadComponent","name","mixins","data","alertHistorico","textoAlerta","tipoAlerta","loader","sortBy","sortDesc","search","headers","text","align","value","sortable","historicos","methods","cancelarReserva","window","requestAlterarStatus","api","id","Cancelada","MotivoCancelamento","Ativo","then","console","catch","finally","created","response","Id","Empresa","Data","Horario","Pessoas","Status"],"sourceRoot":"src/components","sources":["HistoricoClienteComponent.vue"],"sourcesContent":["<template>\r\n    <v-card class=\"pb-3\">\r\n        <alert-component :texto-alerta=\"textoAlerta\" :tipo='tipoAlerta' v-if=\"alertHistorico == true\"/>\r\n        <v-card-title>\r\n            Reservas\r\n            <v-spacer></v-spacer>\r\n            <v-text-field\r\n                v-model=\"search\"\r\n                append-icon=\"mdi-magnify\"\r\n                label=\"Pesquisar\"\r\n                single-line\r\n                hide-details\r\n            ></v-text-field>\r\n        </v-card-title>\r\n        \r\n        <v-card-text>\r\n            <v-data-table\r\n                :search=\"search\"\r\n                :headers=\"headers\"\r\n                :items=\"historicos\"\r\n                :footer-props=\"{\r\n                    'items-per-page-text':'Linhas por pagina',\r\n                    'items-per-page-options': [5, 10, 20, 31]\r\n                }\"\r\n                :header-props=\"{\r\n                    'sortByText': 'Ordenar'\r\n                }\"\r\n                :sort-desc.sync=\"sortDesc\"\r\n                :sort-by.sync=\"sortBy\"\r\n            >\r\n                <template v-slot:item.Status=\"{ item }\">\r\n                    <v-chip\r\n                        :color=\"getColor(item.Status)\"\r\n                        dark\r\n                    >\r\n                        {{ item.Status }}\r\n                    </v-chip>\r\n                </template>\r\n                <template v-slot:item.Empresa=\"{ item }\">\r\n                    <td class=\"font-weight-black\">{{ item.Empresa }}</td>\r\n                </template>\r\n                <template v-slot:item.actions=\"{ item }\">\r\n                    <v-icon\r\n                        color=\"error\"\r\n                        class=\"mr-3\"\r\n                        large\r\n                        @click=\"cancelarReserva(item)\"\r\n                    >\r\n                        mdi-close-circle\r\n                    </v-icon>\r\n                </template>\r\n            </v-data-table>\r\n            <load-component :Ativo=\"loader\"/>\r\n        </v-card-text>\r\n    </v-card>\r\n</template>\r\n<script>\r\n\r\nimport api from '@/services/api';\r\nimport LoadComponent from './LoadComponent.vue';\r\nimport AlertComponent from './AlertComponent.vue';\r\nimport GenericMethods from '@/mixins/GenericMethods';\r\n\r\nexport default {\r\n    components: { AlertComponent, LoadComponent },\r\n\r\n    name: \"HistoricoComponent\",\r\n    mixins: [GenericMethods],\r\n    data: () => ({\r\n        alertHistorico: false,\r\n        textoAlerta: \"\",\r\n        tipoAlerta: \"\",\r\n        loader: false,\r\n        sortBy: 'Id',\r\n        sortDesc: true,\r\n        search: '',\r\n        headers: [\r\n            { text: 'Codigo', align: 'start', value: 'Id',},\r\n            { text: 'Restaurante', align: 'start', value: 'Empresa', },\r\n            { text: 'Data', value: 'Data', },\r\n            { text: 'Horário/Periodo', value: 'Horario', },\r\n            { text: 'Quantidade Pessoas', value: 'Pessoas', },\r\n            { text: 'Status', value: 'Status', },\r\n            { text: 'Ações', value: 'actions', sortable: false}, \r\n            { text: '',  value: 'data-table-expand' },\r\n        ],\r\n        historicos: []\r\n    }),\r\n\r\n    methods: {\r\n        cancelarReserva(item) {\r\n            this.motivoCancelamento = null;\r\n\r\n            if (item.Status == 'Cancelada') {\r\n                this.EnableAlert(\"Reserva já cancelada.\", \"warning\")\r\n                window.scrollTo(0,0);\r\n                return\r\n            }\r\n\r\n            this.acao = \"Cancelar\"\r\n            this.itemEnvio = item\r\n            this.dialog = !this.dialog\r\n        },\r\n        requestAlterarStatus(id, cancelar, motivoCancelamento, ativo) {\r\n            this.loader = !this.loader;\r\n\r\n            api.post('Reserva/AlterarStatus', \r\n            {\r\n                id: id,\r\n                Cancelada: cancelar,\r\n                MotivoCancelamento: motivoCancelamento,\r\n                Ativo: ativo\r\n            })\r\n            .then(response => {\r\n                this.EnableAlert(\"Concluido com sucesso.\", \"success\")\r\n                console.log(response)\r\n            })\r\n            .catch(error => {\r\n                this.EnableAlert(error.message, \"error\")\r\n                window.scrollTo(0,0);\r\n                console.log(error)\r\n            })\r\n            .finally(() => {\r\n                this.loader = !this.loader\r\n                this.requestBuscarPorEmpresaId()\r\n            })\r\n            \r\n        },\r\n    },\r\n    created() {\r\n\r\n        this.loader = !this.loader;\r\n        debugger\r\n\r\n        api.get('Reserva/'+3)\r\n        .then(response => {\r\n            response.data.forEach(element => {\r\n               this.historicos.push({\r\n                    Id: element.id,\r\n                    Empresa: element.empresa.nome,\r\n                    Data: this.FormatDate(new Date(element.dataReserva).toISOString().substring(0,10)),\r\n                    Horario: new Date(element.horario).getHours()+':'+new Date(element.horario).getMinutes(),\r\n                    Pessoas: element.quantidadePessoas,\r\n                    Status: element.cancelada ? 'Cancelada' : element.ativo ? \"Concluida\" : \"Aguardando\"\r\n               })\r\n            });\r\n        })\r\n        .catch(error => {\r\n            this.EnableAlert(error.message, \"error\")\r\n            window.scrollTo(0,0);\r\n            console.log(error)\r\n        })\r\n        .finally(() => (this.loader = !this.loader))\r\n    }\r\n}\r\n</script>\r\n<style>\r\n    \r\n</style>"]},"metadata":{},"sourceType":"module","externalDepe