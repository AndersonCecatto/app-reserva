{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport api from '@/services/api';\nimport LoadComponent from '../LoadComponent.vue';\nimport GenericMethods from '@/mixins/GenericMethods';\nimport DialogPersistentComponent from '../Field/DialogPersistentComponent.vue';\nimport AlertComponent from '../AlertComponent.vue';\nexport default {\n  components: {\n    LoadComponent,\n    DialogPersistentComponent,\n    AlertComponent\n  },\n  name: 'InicialEmpresaComponent',\n  mixins: [GenericMethods],\n  data: () => ({\n    singleExpand: false,\n    itemEnvio: null,\n    motivoCancelamento: null,\n    acao: null,\n    dialog: false,\n    loader: false,\n    sortBy: 'Data',\n    sortDesc: false,\n    search: '',\n    headers: [{\n      text: 'Código',\n      value: 'Id'\n    }, {\n      text: 'Usuario',\n      align: 'start',\n      value: 'Usuario'\n    }, {\n      text: 'Data',\n      value: 'Data'\n    }, {\n      text: 'Horário/Periodo',\n      value: 'Horario'\n    }, {\n      text: 'Quantidade Pessoas',\n      value: 'Pessoas'\n    }, {\n      text: 'Status',\n      value: 'Status'\n    }, {\n      text: 'Actions',\n      value: 'actions',\n      sortable: false\n    }, {\n      text: '',\n      value: 'data-table-expand'\n    }],\n    Reservas: []\n  }),\n  methods: {\n    cancelarReserva(item) {\n      this.motivoCancelamento = null;\n      if (item.Status == 'Cancelada') {\n        this.EnableAlert(\"Reserva já cancelada.\", \"warning\");\n        window.scrollTo(0, 0);\n        return;\n      }\n      this.acao = \"Cancelar\";\n      this.itemEnvio = item;\n      this.dialog = !this.dialog;\n    },\n    requestAlterarStatus(id, cancelar, motivoCancelamento, ativo) {\n      this.loader = !this.loader;\n      api.post('Reserva/AlterarStatus', {\n        id: id,\n        Cancelada: cancelar,\n        MotivoCancelamento: motivoCancelamento,\n        Ativo: ativo\n      }).then(response => {\n        this.EnableAlert(\"Concluido com sucesso.\", \"success\");\n        console.log(response);\n      }).catch(error => {\n        this.EnableAlert(error.message, \"error\");\n        window.scrollTo(0, 0);\n        console.log(error);\n      }).finally(() => {\n        this.loader = !this.loader;\n        this.requestBuscarPorEmpresaId();\n      });\n    },\n    aceitarReserva(item) {\n      if (item.Status == 'Concluida') {\n        this.EnableAlert(\"Reserva já concluida.\", \"warning\");\n        window.scrollTo(0, 0);\n        return;\n      }\n      this.requestAlterarStatus(item.Id, false, null, true);\n    },\n    retornoDialog(retorno) {\n      debugger;\n      if (retorno.acao == \"Cancelar\" && retorno.success) this.requestAlterarStatus(retorno.response.Id, true, this.motivoCancelamento, true);\n      this.dialog = !this.dialog;\n    },\n    requestBuscarPorEmpresaId() {\n      this.loader = !this.loader;\n      this.Reservas = [];\n      debugger;\n      api.get('Reserva/BuscarPorEmpresaId/' + 5).then(response => {\n        response.data.forEach(element => {\n          this.Reservas.push({\n            Id: element.id,\n            Usuario: element.usuario.nome,\n            Data: new Date(element.dataReserva).getDay().toString().padStart(2, '0') + '/' + new Date(element.dataReserva).getMonth().toString().padStart(2, '0') + '/' + new Date(element.dataReserva).getFullYear(),\n            Horario: new Date(element.horario).getHours() + ':' + new Date(element.horario).getMinutes(),\n            Pessoas: element.quantidadePessoas,\n            Status: element.cancelada ? 'Cancelada' : element.ativo ? \"Concluida\" : \"Aguardando\",\n            MotivoCancelamento: element.motivoCancelamento\n          });\n        });\n      }).catch(error => {\n        console.log(error);\n      }).finally(() => this.loader = !this.loader);\n    }\n  },\n  created() {\n    this.requestBuscarPorEmpresaId();\n  }\n};","map":{"version":3,"mappings":";AA0EA;AACA;AACA;AACA;AACA;AAEA;EACAA;IAAAC;IAAAC;IAAAC;EAAA;EAEAC;EACAC;EACAC;IACA