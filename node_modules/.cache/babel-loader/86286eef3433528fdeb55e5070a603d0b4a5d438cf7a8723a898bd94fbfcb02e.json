\n                'items-per-page-text':'Linhas por pagina'\r\n            }\"\r\n            :header-props=\"{\r\n                'sortByText': 'Ordenar'\r\n            }\"\r\n            :sort-desc.sync=\"sortDesc\"\r\n            :sort-by.sync=\"sortBy\"\r\n            :single-expand=\"singleExpand\"\r\n            show-expand\r\n            item-key=\"Id\"\r\n        >\r\n            <template v-slot:item.Status=\"{ item }\">\r\n                <v-chip\r\n                    :color=\"getColor(item.Status)\"\r\n                    dark\r\n                >\r\n                    {{ item.Status }}\r\n                </v-chip>\r\n            </template>\r\n            <template v-slot:item.Empresa=\"{ item }\">\r\n                <td class=\"font-weight-black\">{{ item.Empresa }}</td>\r\n            </template>\r\n            <template v-slot:item.actions=\"{ item }\">\r\n                <v-icon\r\n                    color=\"error\"\r\n                    class=\"mr-3\"\r\n                    @click=\"cancelarReserva(item)\"\r\n                >\r\n                    mdi-close-circle\r\n                </v-icon>\r\n                <v-icon\r\n                    color=\"success\"\r\n                    class=\"ml-3\"\r\n                    @click=\"aceitarReserva(item)\"\r\n                >\r\n                    mdi-check-circle\r\n                </v-icon>\r\n            </template>\r\n            <template v-slot:expanded-item=\"{ headers, item }\">\r\n                <td :colspan=\"headers.length\" v-if=\"item.Status == 'Cancelada'\" >\r\n                    Motivo Cancelamento: {{ MotivoCancelamento }}\r\n                </td>\r\n            </template>\r\n        </v-data-table>\r\n        <load-component :Ativo=\"loader\"/>\r\n        <dialog-persistent-component :item=\"itemEnvio\" titulo=\"Cancelar Reserva\" :dialog=\"dialog\" tamanho=\"400\" @response=\"retornoDialog\" :acao=\"acao\">\r\n            <v-text-field\r\n                v-model=\"motivoCancelamento\"\r\n                label=\"Motivo do Cancelamento\"\r\n            />\r\n        </dialog-persistent-component>\r\n    </v-card>\r\n</template>\r\n<script>\r\n\r\nimport api from '@/services/api'\r\nimport LoadComponent from '../LoadComponent.vue';\r\nimport GenericMethods from '@/mixins/GenericMethods';\r\nimport DialogPersistentComponent from '../Field/DialogPersistentComponent.vue';\r\nimport AlertComponent from '../AlertComponent.vue';\r\n\r\nexport default {\r\n  components: { LoadComponent, DialogPersistentComponent, AlertComponent },\r\n\r\n    name: 'InicialEmpresaComponent',\r\n    mixins: [GenericMethods],\r\n    data: () => ({\r\n        singleExpand: false,\r\n        itemEnvio: null,\r\n        motivoCancelamento: null,\r\n        acao: null,\r\n        dialog: false,\r\n        loader: false,\r\n        sortBy: 'Data',\r\n        sortDesc: false,\r\n        search: '',\r\n        headers: [\r\n            {\r\n                text: 'Código',\r\n                value: 'Id',\r\n            },\r\n            {\r\n                text: 'Usuario',\r\n                align: 'start',\r\n                value: 'Usuario',\r\n            },\r\n            {\r\n                text: 'Data',\r\n                value: 'Data',\r\n            },\r\n            {\r\n                text: 'Horário/Periodo',\r\n                value: 'Horario',\r\n            },\r\n            {\r\n                text: 'Quantidade Pessoas',\r\n                value: 'Pessoas',\r\n            },\r\n            {\r\n                text: 'Status',\r\n                value: 'Status',\r\n            },\r\n            {\r\n                text: 'Actions',\r\n                value: 'actions',\r\n                sortable: false\r\n            },\r\n            {   text: '', \r\n                value: 'data-table-expand' \r\n            },\r\n        ],\r\n        Reservas: []\r\n    }),\r\n\r\n    methods: {\r\n        cancelarReserva(item) {\r\n            this.motivoCancelamento = null;\r\n\r\n            if (item.Status == 'Cancelada') {\r\n                this.EnableAlert(\"Reserva já cancelada.\", \"warning\")\r\n                window.scrollTo(0,0);\r\n                return\r\n            }\r\n\r\n            this.acao = \"Cancelar\"\r\n            this.itemEnvio = item\r\n            this.dialog = !this.dialog\r\n        },\r\n        requestAlterarStatus(id, cancelar, motivoCancelamento, ativo) {\r\n            this.loader = !this.loader;\r\n\r\n            api.post('Reserva/AlterarStatus', \r\n            {\r\n                id: id,\r\n                Cancelada: cancelar,\r\n                MotivoCancelamento: motivoCancelamento,\r\n                Ativo: ativo\r\n            })\r\n            .then(response => {\r\n                this.EnableAlert(\"Concluido com sucesso.\", \"success\")\r\n                console.log(response)\r\n            })\r\n            .catch(error => {\r\n                this.EnableAlert(error.message, \"error\")\r\n                window.scrollTo(0,0);\r\n                console.log(error)\r\n            })\r\n            .finally(() => {\r\n                this.loader = !this.loader\r\n                this.requestBuscarPorEmpresaId()\r\n            })\r\n            \r\n        },\r\n        aceitarReserva(item) {\r\n            if (item.Status == 'Concluida') {\r\n                this.EnableAlert(\"Reserva já concluida.\", \"warning\")\r\n                window.scrollTo(0,0);\r\n                return\r\n            }\r\n\r\n            this.requestAlterarStatus(item.Id, false, null, true)\r\n        },\r\n        retornoDialog(retorno) {\r\n            debugger\r\n            \r\n            if (retorno.acao == \"Cancelar\" && retorno.success) \r\n                this.requestAlterarStatus(retorno.response.Id, true, this.motivoCancelamento, true)\r\n\r\n            this.dialog = !this.dialog\r\n        },\r\n        requestBuscarPorEmpresaId() {\r\n            this.loader = !this.loader;\r\n            \r\n            this.Reservas = []\r\n\r\n            debugger\r\n\r\n            api.get('Reserva/BuscarPorEmpresaId/'+5)\r\n            .then(response => {\r\n                response.data.forEach(element => {\r\n                    this.Reservas.push({\r\n                            Id: element.id,\r\n                            Usuario: element.usuario.nome,\r\n                            Data: new Date(element.dataReserva).getDay().toString().padStart(2, '0')+'/'+new Date(element.dataReserva).getMonth().toString().padStart(2, '0')+'/'+new Date(element.dataReserva).getFullYear(),\r\n                            Horario: new Date(element.horario).getHours()+':'+new Date(element.horario).getMinutes(),\r\n                            Pessoas: element.quantidadePessoas,\r\n                            Status:  element.cancelada ? 'Cancelada' : element.ativo ? \"Concluida\" : \"Aguardando\",\r\n                            MotivoCancelamento: element.motivoCancelamento\r\n                    })\r\n                });\r\n            })\r\n            .catch(error => {\r\n                console.log(error)\r\n            })\r\n            .finally(() => (this.loader = !this.loader))\r\n        }\r\n    },\r\n    \r\n    created() {\r\n        this.requestBuscarPorEmpresaId()\r\n    }\r\n}\r\n</script>\r\n<style>\r\n    \r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}