{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport api from '@/services/api';\nimport LoadComponent from './LoadComponent.vue';\nexport default {\n  components: {\n    LoadComponent\n  },\n  name: 'LoginComponent',\n  data: () => ({\n    alertaUsuario: false,\n    textoAlerta: \"\",\n    corAlerta: \"\",\n    loader: false,\n    senha: \"\",\n    usuario: \"\",\n    show1: false,\n    rules: {\n      required: value => !!value || 'Obrigatório.'\n    }\n  }),\n  methods: {\n    ValidarCampos() {\n      if (this.usuario == \"\") return false;\n      if (this.senha == \"\") return false;\n    },\n    Logar() {\n      this.$refs.form.validate();\n      if (this.ValidarCampos() == false) return;\n      this.loader = !this.loader;\n      api.get('GetUsuario/usuario/' + this.usuario + '/senha/' + this.senha).then(response => {\n        this.loader = !this.loader;\n        if (response.status == 204) {\n          this.alertaUsuario = true;\n          this.textoAlerta = \"Usuário não encontrado.\";\n          setTimeout(() => {\n            this.alertaUsuario = false;\n          }, 1500);\n          return;\n        }\n        this.loader = !this.loader;\n        this.$router.push('/inicial');\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n  },\n  props: {\n    Ativo: Boolean\n  }\n};","map":{"version":3,"mappings":";AAiFA;AACA;AAEA;EACAA;IAAAC;EAAA;EACAC;EAEAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;MACAC;IACA;EACA;EAEAC;IACAC;MACA;MACA;IAEA;IACAC;MAEA;MACA;MAEA;MAEAC,uEACAC;QAEA;QAEA;UACA;UACA;UACAC;YACA;UACA;UAEA;QACA;QAEA;QACA;MACA,GACAC;QACAC;MACA;IACA;EACA;EACAC;IACAC;EACA;AACA","names":["components","LoadComponent","name","data","alertaUsuario","textoAlerta","corAlerta","loader","senha","usuario","show1","rules","required","methods","ValidarCampos","Logar","api","then","setTimeout","catch","console","props","Ativo"],"sourceRoot":"src/components","sources":["LoginComponent.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <v-alert \r\n            type=\"warning\" \r\n            class=\"alert\"\r\n            dense\r\n            border=\"left\"\r\n            v-if=\"alertaUsuario == true\"\r\n            >\r\n            {{ textoAlerta }}\r\n        </v-alert>\r\n        <v-container class=\"text-center\">\r\n            <v-row class=\"text-center\">\r\n                <v-col cols=\"12\" \r\n                    sm=\"1\"\r\n                    xs=\"1\"\r\n                    md=\"3\"\r\n                    />\r\n                <v-col cols=\"12\"\r\n                    md=\"6\"\r\n                    xs=\"10\"\r\n                    sm=\"10\"\r\n                >\r\n                    <v-card\r\n                        elevation=\"2\"\r\n                        class=\"px-6 top\"\r\n                        xs=\"top-mobile\"\r\n                        sm=\"top-mobile\"\r\n                    >\r\n                        <v-card-title >Faça seu Login</v-card-title>\r\n                        <v-card-text>\r\n                            <v-container>\r\n                                <v-form\r\n                                    ref=\"form\" \r\n                                    lazy-validation\r\n                                >\r\n                                    <v-row  class=\"px-6\">\r\n                                        <v-text-field\r\n                                        v-model=\"usuario\"\r\n                                        label=\"Usuário\"\r\n                                        :rules=\"[rules.required]\"\r\n                                        color=\"green\"\r\n                                        required\r\n                                        />\r\n                                    </v-row>\r\n                                    <v-row  class=\"px-6 xs-6\">\r\n                                        <v-text-field\r\n                                            v-model=\"senha\"\r\n                                            :append-icon=\"show1 ? 'mdi-eye' : 'mdi-eye-off'\"\r\n                                            :rules=\"[rules.required]\"\r\n                                            :type=\"show1 ? 'text' : 'password'\"\r\n                                            label=\"Senha\"\r\n                                            color=\"green\"\r\n                                            counter\r\n                                            required\r\n                                            @click:append=\"show1 = !show1\"\r\n                                        ></v-text-field>\r\n                                    </v-row>\r\n                                </v-form>\r\n                                <v-row class=\"px-6 pt-6\">\r\n                                    <v-btn\r\n                                        outlined\r\n                                        large\r\n                                        block\r\n                                        color=\"green\"\r\n                                        @click=\"Logar()\"\r\n                                        >\r\n                                        Continuar\r\n                                    </v-btn>\r\n                                </v-row>\r\n                            </v-container>\r\n                        </v-card-text>\r\n                    </v-card>\r\n                </v-col>\r\n            </v-row>\r\n        </v-container>\r\n        <load-component :Ativo=\"loader\"/>\r\n    </div>\r\n</template>\r\n<script>\r\n\r\nimport api from '@/services/api';\r\nimport LoadComponent from './LoadComponent.vue'\r\n\r\nexport default {\r\n  components: { LoadComponent },\r\n    name: 'LoginComponent',\r\n\r\n    data: () =>({\r\n        alertaUsuario: false,\r\n        textoAlerta: \"\",\r\n        corAlerta: \"\",\r\n        loader: false,\r\n        senha: \"\",\r\n        usuario: \"\",\r\n        show1: false,\r\n        rules: {\r\n            required: value => !!value || 'Obrigatório.'\r\n        },\r\n    }),\r\n\r\n    methods: {\r\n        ValidarCampos() {\r\n            if (this.usuario == \"\") return false\r\n            if (this.senha == \"\") return false\r\n\r\n        },\r\n        Logar() {\r\n            \r\n            this.$refs.form.validate()\r\n            if (this.ValidarCampos() == false) return\r\n\r\n            this.loader = !this.loader;\r\n            \r\n            api.get('GetUsuario/usuario/'+this.usuario+'/senha/'+this.senha)\r\n            .then(response => {\r\n\r\n                this.loader = !this.loader;\r\n\r\n                if (response.status == 204) {\r\n                    this.alertaUsuario = true\r\n                    this.textoAlerta = \"Usuário não encontrado.\"\r\n                    setTimeout(() => {\r\n                        this.alertaUsuario = false\r\n                    }, 1500)\r\n\r\n                    return \r\n                }\r\n\r\n                this.loader = !this.loader;\r\n                this.$router.push('/inicial')\r\n            })\r\n            .catch(error => {\r\n                console.log(error)\r\n            })\r\n        }\r\n    },\r\n    props: {\r\n        Ativo: Boolean\r\n    }\r\n}\r\n</script>\r\n<style>\r\n   .top {\r\n    margin-top: 25% !important;\r\n   }\r\n\r\n@media only screen and (max-width: 600px) {\r\n        .top {\r\n            margin-top: 55% !important;\r\n        }\r\n    }\r\n\r\n    .login {\r\n        background-color: #81C784;\r\n    }\r\n\r\n    .alert {\r\n        position: absolute;\r\n        width: -webkit-fill-available;\r\n    }\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}