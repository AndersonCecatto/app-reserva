{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport api from '@/services/api';\nimport LoadComponent from '../LoadComponent.vue';\nimport GenericMethods from '@/mixins/GenericMethods';\nimport DialogPersistentComponent from '../Field/DialogPersistentComponent.vue';\nimport AlertComponent from '../AlertComponent.vue';\nexport default {\n  components: {\n    LoadComponent,\n    DialogPersistentComponent,\n    AlertComponent\n  },\n  name: 'InicialEmpresaComponent',\n  mixins: [GenericMethods],\n  data: () => ({\n    itemEnvio: null,\n    motivoCancelamento: null,\n    acao: null,\n    dialog: false,\n    loader: false,\n    sortBy: 'Data',\n    sortDesc: false,\n    search: '',\n    headers: [{\n      text: 'Restaurante',\n      align: 'start',\n      value: 'Empresa'\n    }, {\n      text: 'Data',\n      value: 'Data'\n    }, {\n      text: 'Horário/Periodo',\n      value: 'Horario'\n    }, {\n      text: 'Status',\n      value: 'Status'\n    }, {\n      text: 'Actions',\n      value: 'actions',\n      sortable: false\n    }],\n    Reservas: []\n  }),\n  methods: {\n    cancelarReserva(item) {\n      this.acao = \"Cancelar\";\n      this.itemEnvio = item;\n      this.dialog = !this.dialog;\n    },\n    requestCancelar(item) {\n      this.loader = !this.loader;\n      api.post('Reserva/CancelarReserva', {\n        id: item.Id,\n        Cancelada: true,\n        MotivoCancelamento: this.motivoCancelamento\n      }).then(response => {\n        this.EnableAlert(\"Concluido com sucesso.\", \"success\");\n        console.log(response);\n      }).catch(error => {\n        this.EnableAlert(error.message, \"error\");\n        console.log(error);\n      }).finally(() => this.loader = !this.loader);\n    },\n    aceitarReserva(item) {\n      this.acao = \"Aceitar\";\n      this.dialog = !this.dialog;\n    },\n    requestAceitar() {},\n    retornoDialog(retorno) {\n      debugger;\n      if (retorno.acao == \"Cancelar\") {\n        this.requestCancelar(retorno.response);\n      }\n      this.dialog = !this.dialog;\n    },\n    requestBuscarPorEmpresaId() {\n      this.loader = !this.loader;\n      api.get('Reserva/BuscarPorEmpresaId/' + 5).then(response => {\n        response.data.forEach(element => {\n          this.Reservas.push({\n            Id: element.id,\n            Empresa: element.empresa.nome,\n            Data: new Date(element.dataReserva).getDay().toString().padStart(2, '0') + '/' + new Date(element.dataReserva).getMonth().toString().padStart(2, '0') + '/' + new Date(element.dataReserva).getFullYear(),\n            Horario: new Date(element.horario).getHours() + ':' + new Date(element.horario).getMinutes(),\n            Status: element.ativo && !element.cancelada ? 'Concluida' : 'Cancelada'\n          });\n        });\n      }).catch(error => {\n        console.log(error);\n      }).finally(() => {\n        this.loader = !this.loader;\n        this.requestBuscarPorEmpresaId();\n      });\n    }\n  },\n  created() {\n    this.requestBuscarPorEmpresaId();\n  }\n};","map":{"version":3,"mappings":";AAkEA;AACA;AACA;AACA;AACA;AAEA;EACAA;IAAAC;IAAAC;IAAAC;EAAA;EAEAC;EACAC;EACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC,UACA;MACAC;MACAC;MACAC;IACA,GACA;MACAF;MACAE;IACA,GACA;MACAF;MACAE;IACA,GACA;MACAF;MACAE;IACA,GACA;MACAF;MACAE;MACAC;IACA,EACA;IACAC;EACA;EAEAC;IACAC;MACA;MACA;MACA;IACA;IACAC;MACA;MAEAC,oCACA;QACAC;QACAC;QACAC;MACA,GACAC;QACA;QACAC;MACA,GACAC;QACA;QACAD;MACA,GACAE;IACA;IACAC;MACA;MACA;IACA;IACAC,kBAEA;IACAC;MACA;MAEA;QACA;MACA;MAEA;IACA;IACAC;MACA;MAEAX,2CACAI;QACAQ;UACA;YACAC;YACAC;YACAC;YACAC;YACAC;UACA;QACA;MACA,GACAX;QACAD;MACA,GACAE;QACA;QACA;MACA;IACA;EACA;EAEAW;IACA;EACA;AACA","names":["components","LoadComponent","DialogPersistentComponent","AlertComponent","name","mixins","data","itemEnvio","motivoCancelamento","acao","dialog","loader","sortBy","sortDesc","search","headers","text","align","value","sortable","Reservas","methods","cancelarReserva","requestCancelar","api","id","